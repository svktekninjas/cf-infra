# Harness CLI Configuration
# Generated by Ansible

api_key: {{ harness_api_key }}
account_id: {{ harness_account_id }}
org_id: {{ harness_org_id }}
project_id: {{ harness_project_id }}
platform_url: {{ harness_platform_url }}
api_endpoint: {{ harness_api_endpoint }}

# Default settings
default_timeout: 300
output_format: json
verify_ssl: true

# Environment specific settings
environment: {{ environment }}
region: {{ aws_region }}

# Feature flags
enable_debug: {{ debug_mode | bool | lower }}
enable_verbose: {{ verbose_output | bool | lower }}